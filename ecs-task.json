{
  "family": "ecs-task1",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::787169319732:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "ecs-container",
      "image": "787169319732.dkr.ecr.ap-northeast-1.amazonaws.com/ecs-repo1:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
